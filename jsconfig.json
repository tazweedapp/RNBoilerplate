{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@screens/*": ["screens/*"],
      "@navigation/*": ["navigation/*"],
      "@redux/*": ["redux/*"],
      "@constants/*": ["constants/*"],
      "@components/*": ["components/*"],
      "@locales/*": ["locales/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
